{
    "packageManager": "yarn@1.22.19",
    "name": "@acaldas/document-model-libs",
    "version": "1.0.0",
    "main": "dist/index.js",
    "module": "dist/es/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./budget-statement": "./dist/budget-statement.js",
        "./document": "./dist/document.js"
    },
    "typesVersions": {
        "*": {
            "budget-statement": [
                "dist/budget-statement"
            ],
            "document": [
                "dist/document"
            ]
        }
    },
    "files": [
        "/dist"
    ],
    "repository": "https://github.com/makerdao-ses/document-model-libs",
    "author": "acaldas",
    "license": "AGPL-3.0-only",
    "private": false,
    "scripts": {
        "build": "rollup --config rollup.config.ts --configPlugin typescript",
        "lint": "eslint --ext .js,.ts .",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "test": "jest",
        "docs": "typedoc src/index.doc.ts --plugin none",
        "docs:markdown": "typedoc src/index.doc.ts --plugin typedoc-plugin-markdown --out markdown"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/jest": "^29.4.0",
        "@types/mime": "^3.0.1",
        "@types/node": "^18.14.6",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "esbuild": "^0.17.11",
        "eslint": "^8.35.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-tsdoc": "^0.2.17",
        "jest": "^29.4.3",
        "jsonc-parser": "^3.2.0",
        "prettier": "^2.8.4",
        "prettier-plugin-organize-imports": "^3.2.2",
        "rollup": "^3.19.1",
        "rollup-plugin-dts": "^5.2.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "ts-jest": "^29.0.5",
        "typedoc": "^0.23.28",
        "typedoc-plugin-markdown": "^3.14.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "immer": "^9.0.19",
        "jszip": "^3.10.1",
        "mime": "^3.0.0"
    }
}
